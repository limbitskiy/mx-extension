{
    "name": "MyHotelka's",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-scripts/main.js"
            ],
            "css": [
                "content-scripts/main.e0581493.css"
            ]
        }
    ],
    "permissions": [
        "alarms",
        "storage"
    ]
}